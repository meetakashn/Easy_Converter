<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/currencyback"
    tools:context=".FLength">


    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_55sdp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="@dimen/_0sdp"
        android:layout_marginTop="@dimen/_21sdp"
        android:background="#FFB300" />

    <EditText
        android:id="@+id/value1"
        android:layout_width="@dimen/_140sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="@dimen/_8sdp"
        android:layout_marginTop="@dimen/_34sdp"
        android:layout_marginEnd="@dimen/_10sdp"
        android:layout_toStartOf="@+id/spin1"
        android:autofillHints=""
        android:background="#FF7070"
        android:backgroundTint="#3949AB"
        android:drawableEnd="@android:drawable/ic_menu_edit"
        android:ems="10"
        android:gravity="center"
        android:hint="@string/value"
        android:inputType="number"
        android:textColorHint="#FFFFFF"
        android:textSize="@dimen/_20ssp"
        tools:ignore="TouchTargetSizeCheck,VisualLintTextFieldSize,TextContrastCheck" />

    <Spinner
        android:id="@+id/spin1"
        android:layout_width="@dimen/_150sdp"
        android:layout_height="@dimen/_38sdp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="@dimen/_32sdp"
        android:layout_marginEnd="@dimen/_11sdp"
        android:background="@android:drawable/btn_dropdown"
        android:contentDescription="@string/Select"
        android:dropDownWidth="match_parent"
        android:dropDownSelector="@android:drawable/arrow_down_float"
        android:popupBackground="#F60000"
        app:layout_constraintVertical_bias="0.688"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="@dimen/_155sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_alignTop="@+id/textView10"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="@dimen/_36sdp"
        android:layout_marginEnd="@dimen/_0sdp"
        android:background="#C8BCBC"
        android:fadingEdge="horizontal"
        android:gravity="left"
        android:shadowColor="#000000"
        android:text="   AUD"
        android:textColor="#000000"
        android:textSize="@dimen/_15ssp"
        tools:ignore="TextContrastCheck"
        app:drawableEndCompat="@drawable/australian_dollar_icon" />

    <TextView
        android:id="@+id/AUD"
        android:layout_width="@dimen/_155sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_alignTop="@+id/GBP"
        android:layout_alignParentStart="true"
        android:layout_marginStart="@dimen/_0sdp"
        android:layout_marginTop="@dimen/_36sdp"
        android:layout_marginEnd="@dimen/_8sdp"
        android:layout_toStartOf="@+id/textView13"
        android:background="#565353"
        android:fontFamily="sans-serif-condensed"
        android:gravity="right"
        android:text="000.00"
        android:textColor="#FFFFFF"
        android:textSize="@dimen/_15ssp" />

    <TextView
        android:id="@+id/EUR"
        android:layout_width="@dimen/_155sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_alignTop="@+id/USD"
        android:layout_alignParentStart="true"
        android:layout_marginStart="@dimen/_0sdp"
        android:layout_marginTop="@dimen/_36sdp"
        android:layout_marginEnd="@dimen/_7sdp"
        android:layout_toStartOf="@+id/textView4"
        android:background="#F50057"
        android:fontFamily="sans-serif-condensed"
        android:gravity="right"
        android:text="@string/_000_00"
        android:textColor="#FFFFFF"
        android:textSize="@dimen/_15ssp"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="@dimen/_156sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_alignTop="@+id/textView6"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="@dimen/_36sdp"
        android:layout_marginEnd="@dimen/_0sdp"
        android:background="#F50057"
        android:drawableRight="@drawable/euro"
        android:fadingEdge="horizontal"
        android:gravity="left"
        android:shadowColor="#000000"
        android:text="   EUR"
        android:textColor="#FFFFFF"
        android:textSize="@dimen/_15ssp"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/USD"
        android:layout_width="@dimen/_155sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_alignTop="@+id/INR1"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="@dimen/_0sdp"
        android:layout_marginTop="@dimen/_155sdp"
        android:layout_marginEnd="@dimen/_7sdp"
        android:layout_toStartOf="@+id/textView6"
        android:background="#565353"
        android:fontFamily="sans-serif-condensed"
        android:gravity="right"
        android:text="000.00"
        android:textColor="#FFFFFF"
        android:textSize="@dimen/_15ssp" />

    <TextView
        android:id="@+id/CAD"
        android:layout_width="@dimen/_155sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_alignTop="@+id/AUD"
        android:layout_alignParentStart="true"
        android:layout_marginStart="@dimen/_0sdp"
        android:layout_marginTop="@dimen/_36sdp"
        android:layout_marginEnd="@dimen/_7sdp"
        android:layout_toStartOf="@+id/textView12"
        android:background="#F50057"
        android:fontFamily="sans-serif-condensed"
        android:gravity="right"
        android:text="000.00"
        android:textColor="#FFFFFF"
        android:textSize="@dimen/_15ssp"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="@dimen/_155sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_alignTop="@+id/textView4"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="@dimen/_36sdp"
        android:layout_marginEnd="@dimen/_1sdp"
        android:background="#C8BCBC"
        android:drawableRight="@drawable/yen_coin_solid_icon__1_"
        android:fadingEdge="horizontal"
        android:gravity="left"
        android:shadowColor="#000000"
        android:text="   JPY"
        android:textColor="#000000"
        android:textSize="@dimen/_15ssp"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="@dimen/_155sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_alignTop="@+id/textView11"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="@dimen/_36sdp"
        android:layout_marginEnd="@dimen/_0sdp"
        android:background="#F50057"
        android:drawableRight="@drawable/gbp"
        android:fadingEdge="horizontal"
        android:gravity="left"
        android:shadowColor="#000000"
        android:text="   GBP"
        android:textColor="#FFFFFF"
        android:textSize="@dimen/_15ssp"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/GBP"
        android:layout_width="@dimen/_155sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_alignTop="@+id/JPY"
        android:layout_alignParentStart="true"
        android:layout_marginStart="@dimen/_0sdp"
        android:layout_marginTop="@dimen/_36sdp"
        android:layout_marginEnd="@dimen/_8sdp"
        android:layout_toStartOf="@+id/textView10"
        android:background="#F50057"
        android:fontFamily="sans-serif-condensed"
        android:gravity="right"
        android:text="000.00"
        android:textColor="#FFFFFF"
        android:textSize="@dimen/_15ssp"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/JPY"
        android:layout_width="@dimen/_155sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_alignTop="@+id/EUR"
        android:layout_alignParentStart="true"
        android:layout_marginStart="@dimen/_0sdp"
        android:layout_marginTop="@dimen/_36sdp"
        android:layout_marginEnd="@dimen/_7sdp"
        android:layout_toStartOf="@+id/textView11"
        android:background="#565353"
        android:fontFamily="sans-serif-condensed"
        android:gravity="right"
        android:text="000.00"
        android:textColor="#FBFBFB"
        android:textSize="@dimen/_15ssp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="@dimen/_155sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_alignTop="@+id/textView13"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="@dimen/_36sdp"
        android:layout_marginEnd="@dimen/_1sdp"
        android:background="#F50057"
        android:drawableRight="@drawable/canadian_dollar_svgrepo_com"
        android:fadingEdge="horizontal"
        android:gravity="left"
        android:shadowColor="#000000"
        android:text="   CAD"
        android:textColor="#FFFFFF"
        android:textSize="@dimen/_15ssp"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="@dimen/_156sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_alignTop="@+id/textView5"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="@dimen/_36sdp"
        android:layout_marginEnd="@dimen/_0sdp"
        android:background="#C8BCBC"
        android:drawableRight="@drawable/baseline_monetization_on_24"
        android:fadingEdge="horizontal"
        android:gravity="left"
        android:shadowColor="#000000"
        android:text="   USD"
        android:textColor="#040303"
        android:textSize="@dimen/_15ssp"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="@dimen/_156sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_below="@+id/spin1"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="@dimen/_49sdp"
        android:layout_marginEnd="@dimen/_0sdp"
        android:background="#F50057"
        android:drawableRight="@drawable/indian_rupee_symbol"
        android:fadingEdge="horizontal"
        android:gravity="left"
        android:shadowColor="#000000"
        android:text="   INR"
        android:textColor="#FFFFFF"
        android:textSize="@dimen/_15ssp"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/INR1"
        android:layout_width="@dimen/_155sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_below="@+id/value1"
        android:layout_alignParentStart="true"
        android:layout_marginStart="@dimen/_0sdp"
        android:layout_marginTop="@dimen/_55sdp"
        android:layout_marginEnd="@dimen/_7sdp"
        android:layout_toStartOf="@+id/textView5"
        android:background="#F50057"
        android:fontFamily="sans-serif-condensed"
        android:gravity="right"
        android:text="000.00"
        android:textColor="#FFFFFF"
        android:textSize="@dimen/_15ssp"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/button"
        android:layout_width="@dimen/_117sdp"
        android:layout_height="@dimen/_40sdp"
        android:layout_below="@+id/textView3"
        android:layout_alignParentStart="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="@dimen/_35sdp"
        android:layout_marginTop="@dimen/_300sdp"
        android:backgroundTint="#FF9100"
        android:text="Convert"
        android:textColor="#000000"
        app:icon="@drawable/baseline_sync_24"
        app:iconTint="#000000" />
    <Button
        android:id="@+id/clear"
        android:layout_width="@dimen/_70sdp"
        android:layout_height="@dimen/_40sdp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="@dimen/_162sdp"
        android:layout_marginTop="@dimen/_376sdp"
        android:backgroundTint="#FF0000"
        android:gravity="center"
        android:text="Clear"
        android:textColorHighlight="#00FFFFFF"
        app:iconTint="#FFFFFF" />

</RelativeLayout>
